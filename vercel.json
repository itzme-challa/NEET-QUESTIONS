{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/node"
    },
    {
      "src": "build/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/build/static/$1",
      "status": 200
    },
    {
      "src": "/data/(.*)",
      "dest": "/build/data/$1",
      "status": 200
    },
    {
      "src": "/(.*)",
      "dest": "/build/index.html",
      "status": 200
    }
  ]
}
